@Article{Schreiber2020a,
  author       = {Schreiber, Jacob and Hegde, Deepthi and Noble, William},
  journal      = {bioRxiv},
  title        = {Zero-shot imputations across species are enabled through joint modeling of human and mouse epigenomics},
  year         = {2020},
  pages        = {10.1101/801183},
  abstract     = {Recent large-scale efforts to characterize functional activity in human have produced thousands of genome-wide experiments that quantify various forms of biochemistry, such as histone modifications, protein binding, transcription, and chromatin accessibility. Although these experiments represent a small fraction of the possible experiments that could be performed, they also make human more comprehensively characterized than any other species. We propose an extension to the imputation approach Avocado that enables the model to leverage genome alignments and the large number of human genomics data sets when making imputations in other species. We found that not only does this extension result in improved imputation of mouse functional experiments, but that the extended model is able to make accurate imputations for protein binding assays that have been performed in human but not in mouse. This ability to make {\textquotedblleft}zero-shot{\textquotedblright} imputations greatly increases the utility of such imputation approaches and enables comprehensive imputations to be made for species even when experimental data are sparse.CCS CONCEPTS {\textbullet} Computing methodologies {\textrightarrow} Neural networks; Factorization methods; {\textbullet} Applied computing {\textrightarrow} Bioinformatics; Genomics.ACM Reference Format Jacob Schreiber, Deepthi Hegde, and William Noble. 2020. Zero-shot imputations across species are enabled through joint modeling of human and mouse epigenomics. In ACM-BCB 2020: 11th ACM Conference on Bioinformatics, Computational Biology, and Health Informatics, Sept 21{\textendash}24, 2020, Virtual. ACM, New York, NY, USA, 9 pages. https://doi.org/10.1145/1122445.1122456Competing Interest StatementThe authors have declared no competing interest.},
  doi          = {10.1101/801183},
  elocation-id = {801183},
