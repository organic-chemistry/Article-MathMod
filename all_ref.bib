  keywords        = {Animals; Cells, Cultured; Chromatin, metabolism; Chromosomes, ultrastructure; DNA Replication, drug effects; Drosophila melanogaster; Floxuridine, pharmacology; Heterochromatin, metabolism; Mitosis; Time Factors},
  nlm-id          = {2985138R},
